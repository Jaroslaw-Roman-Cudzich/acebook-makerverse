<h1 class="profile">Profile of {{user.name}}</h1>

 <img id="profile-pic" src="data:image/{{image.img.contentType}};base64,{{profilePic}}" class="shrinkToFit" width="360" height="360">


<h1>Upload Image</h1>
<form action="/users/upload/" enctype="multipart/form-data" method="POST">
    <input type="file" name="image" accept="image/*">
    <input type="submit" value="Upload Photo">
</form>

<h1>Timeline</h1>
{{> postList }}

